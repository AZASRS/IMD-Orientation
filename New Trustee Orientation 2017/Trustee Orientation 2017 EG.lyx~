\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
2 principal strategies: fundamental and quantitative
\end_layout

\begin_deeper
\begin_layout Itemize
Fundamental managers: bottoms-up stock picking in value and growth strategies
\end_layout

\begin_deeper
\begin_layout Itemize
Wellington, TimesSquare, Brandes, American Century, Trinity Street, Thompson
 Siegel Walmsley, Franklin Templeton, and William Blair
\end_layout

\end_deeper
\begin_layout Itemize
Quantitative managers: factor based stock picking utilizing value, momentum,
 size, minimum volatility, and quality
\end_layout

\begin_deeper
\begin_layout Itemize
LSV, DFA, AQR, and Eaton Vance
\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
ASRS is studying the implementation of active management
\end_layout

