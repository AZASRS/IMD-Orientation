pe.port.df=data.frame(pe.port.names,pe.port.pmes,pe.port.Fund.IRR,pe.port.R2K.DM.IRR,pe.port.Fun
d.TVPI) 
\end_layout

\begin_layout Plain Layout

colnames(pe.port.df) = c("Fund","R2K PME","Fund IRR","R2K $Mtch IRR","Fund
 TVPI") 
\end_layout

